{
  "name": "frontend",
  "version": "1.6.4",
  "description": "GÖKÇE Pansiyon Yönetim Sistemi",
  "productName": "GÖKÇE Pansiyon",
  "author": "HaBiRetwiTT <habiretwitt@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "postinstall": "node -e \"if (process.platform !== 'win32') require('child_process').execSync('chmod +x node_modules/.bin/*')\"",
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "kill-port": "powershell -ExecutionPolicy Bypass -File \"../kill-port-9000.ps1\"",
    "dev": "npm run kill-port && quasar dev",
    "dev:clean": "npm run kill-port && quasar dev",
    "prebuild": "node scripts/update-version.js",
    "build": "quasar build",
    "build:vercel": "quasar build",
    "type-check": "vue-tsc --noEmit",
    "build:strict": "npm run type-check && quasar build",
    "dev:strict": "npm run type-check && quasar dev",
    "start": "serve -s dist/spa -l 9000",
    "start:prod": "npm run build && npm run start"
  },
  "dependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.3.0",
    "@quasar/cli": "^2.5.0",
    "@quasar/extras": "^1.16.4",
    "@vercel/node": "^3.0.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.2",
    "axios": "^1.2.1",
    "basic-ftp": "^5.0.5",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "pinia": "^3.0.1",
    "vite-plugin-checker": "^0.9.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12",
    "vue-tsc": "^2.0.29",
    "chart.js": "^4.4.4"
  },
  "devDependencies": {
    "@types/node": "^20.5.9",
    "electron-builder": "^26.0.12",
    "prettier": "^3.3.3",
    "quasar": "^2.18.2",
    "typescript": "~5.5.3"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
