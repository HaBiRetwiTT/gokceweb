    "vue": "^3.4.18",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.2.1",
    "@quasar/cli": "^2.5.0",
    "@quasar/extras": "^1.16.4",
    "@types/node": "^20.5.9",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
